{

  "description": "Sends a request to deployed endpoint to classify something by genre",
  "manifest_version": 2,
  "name": "Classify",
  "version": "1.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
  "icons": {
    "48": "icons/pen.jpg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/pen.png",
    "default_title": "Classifier",
    "default_popup": "popup/classifier.html"
  }
}
